@page
@model CodeFirstCrud.Views.Shared.ErrorModel
@{

}
<h1>
    An Error occur while processing your Request.
   The support team is notified , We are working in the issue
</h1>
<h2> Please Contact us on the Satyamp@Chetu.com</h2>

    <div class=" alert alert-danger">
        <h5> Exception Path</h5>
        <hr />
        <p>
            @ViewBag.ExceptionPath; 
        </p>
    </div>

    <div class=" alert alert-danger">
        <h5> Exception Message</h5>
        <hr />
        <p>
            @ViewBag.ExceptionMessage; 
        </p>
    </div>

    <div class=" alert alert-danger">
        <h5> Exception Trace</h5>
        <hr />
        <p>
            @ViewBag.StackTrace; 
        </p>
    </div>


   

